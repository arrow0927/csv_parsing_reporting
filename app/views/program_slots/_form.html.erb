<%= simple_form_for @program_slot, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :program_id %>
  <%= f.input :market_id %>
  <%= f.input :channel_id %>
  <%= f.input :infosys_start %>
  <%= f.input :infosys_end %>
  <%= f.input :infosys_airdate %>
  <%= f.input :infosys_day %>
  <%= f.input :calendar_start %>
  <%= f.input :calendar_end %>
  <%= f.input :calendar_airdate %>
  <%= f.input :timeslot %>
  <%= f.input :normalized_start_time %>
  <%= f.input :normalized_date %>
  <%= f.input :normalized_end_time %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                program_slots_path, :class => 'btn' %>
  </div>
<% end %>
