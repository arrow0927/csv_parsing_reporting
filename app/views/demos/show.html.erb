<%- model_class = Demo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:program_slot_id) %>:</strong></dt>
  <dd><%= @demo.program_slot_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:metric_type) %>:</strong></dt>
  <dd><%= @demo.metric_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Ind.2+) %>:</strong></dt>
  <dd><%= @demo.Ind.2+ %></dd>
  <dt><strong><%= model_class.human_attribute_name(:A18+) %>:</strong></dt>
  <dd><%= @demo.A18+ %></dd>
  <dt><strong><%= model_class.human_attribute_name(:A25-54) %>:</strong></dt>
  <dd><%= @demo.A25-54 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:A18-49) %>:</strong></dt>
  <dd><%= @demo.A18-49 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:A18-34) %>:</strong></dt>
  <dd><%= @demo.A18-34 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:A55+) %>:</strong></dt>
  <dd><%= @demo.A55+ %></dd>
  <dt><strong><%= model_class.human_attribute_name(:F25-54) %>:</strong></dt>
  <dd><%= @demo.F25-54 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:F18-49) %>:</strong></dt>
  <dd><%= @demo.F18-49 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:F18-34) %>:</strong></dt>
  <dd><%= @demo.F18-34 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:M25-54) %>:</strong></dt>
  <dd><%= @demo.M25-54 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:M18-49) %>:</strong></dt>
  <dd><%= @demo.M18-49 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:M18-34) %>:</strong></dt>
  <dd><%= @demo.M18-34 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:A12-34) %>:</strong></dt>
  <dd><%= @demo.A12-34 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:T12-17) %>:</strong></dt>
  <dd><%= @demo.T12-17 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:C2-11) %>:</strong></dt>
  <dd><%= @demo.C2-11 %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              demos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_demo_path(@demo), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              demo_path(@demo),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
